    "@angular/forms": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/router": "^20.0.0",
    "rxjs": "~7.8.0",
    "sass": "^1.89.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.1",
    "@angular/build": "^20.0.1",
    "@angular/cli": "^20.0.1",
    "@angular/compiler-cli": "^20.0.0",
    "@angular/platform-browser-dynamic": "^20.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.0.0",
