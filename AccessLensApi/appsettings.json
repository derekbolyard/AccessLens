{
  "ConnectionStrings": {
    "Sqlite": "Data Source=accesslens.db"
  },
  "Urls": {
    "BaseUrl": "https://localhost:7088/api",
    "WebAppUrl": "http://localhost:4200"
  },
  "Playwright": {
    "Browser": "chromium",
    "Headless": true,
    "Args": [ "--disable-dev-shm-usage" ],
    "BrowsersPath": "/ms-playwright"
  },
  "Turnstile": {
    "SecretKey": ""
  },
  "Minio": {
    "User": "minioadmin",
    "Password": ""
  },
  "RateLimitingOptions": {
    "WindowMinutes": 60,
    "MaxStarterScansPerHour": 3,
    "MaxUnverifiedScansPerHour": 1,
    "MaxScansPerEmailPerDay": 5,
    "MaxConcurrentScans": 5,
    "MaxConcurrentFullScans": 2,
    "MaxFullScansPerIpPerDay": 2,
    "MaxFullScansPerEmailPerDay": 3,
    "MaxScanDurationSeconds": 30,
    "MaxFullScanDurationMinutes": 30
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "SendGrid": {
    "ApiKey": "",
    "FromEmail": "derek@accesslens.local"
  },
  "S3": {
    "BucketName": "accesslens-reports",
    "ServiceUrl": "http://localhost:9000",
    "Region": "us-east-1"
  },
  "Jwt": {
    "Issuer": "AccessLens",
    "Audience": "Access",
    "SecretKey": ""
  },
    "AllowedHosts": "*"
  }
