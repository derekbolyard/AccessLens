{
  "ConnectionStrings": {
    "DefaultConnection": "Server=YOUR_SQL_SERVER;Database=AccessLensDb;User Id=YOUR_USER;Password=YOUR_PASSWORD;TrustServerCertificate=True;"
  },
  "AWS": {
    "Region": "us-east-1",
    "S3Bucket": "accesslens-reports",
    "SesFromEmail": "no-reply@yourdomain.com"
  },
  "Stripe": {
    "SecretKey": "sk_test_YourStripeSecretKey",
    "WebhookSecret": "whsec_YourStripeWebhookSecret"
  },
  "Playwright": {
    "Headless": true,
    "Args": [ "--disable-dev-shm-usage" ]
  },
  "Captcha": {
    "hCaptchaSecret": "0x0000000000000000000000000000000000000000"
  },
  "RateLimiting": {
    "MaxUnverifiedScansPerHour": 10,
    "WindowMinutes": 60
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
