{
  "ConnectionStrings": {
    "SqliteConnection": "Data Source=accesslens.db"
  },
  "Urls": {
    "BaseUrl": "https://localhost:7088/api",
    "WebAppUrl": "http://localhost:4200"
  },
  "AWS": {
    "Region": "",
    "S3Bucket": ""
  },
  "Playwright": {
    "Headless": true,
    "Args": [ "--disable-dev-shm-usage" ]
  },
  "Turnstile": {
    "SecretKey": ""
  },
  "RateLimitingOptions": {
    "WindowMinutes": 60,
    "MaxStarterScansPerHour": 3,
    "MaxUnverifiedScansPerHour": 1,
    "MaxScansPerEmailPerDay": 5,
    "MaxConcurrentScans": 5,
    "MaxConcurrentFullScans": 2,
    "MaxFullScansPerIpPerDay": 2,
    "MaxFullScansPerEmailPerDay": 3,
    "MaxScanDurationSeconds": 30,
    "MaxFullScanDurationMinutes": 30
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "SendGrid": {
    "ApiKey": "<your-sendgrid-api-key>",
    "FromEmail": "noreply@yourdomain.com"
  },
  "Gcs": {
    "BucketName": "",
    "ServiceAccountJson": ""
  },
  "AllowedHosts": "*"
}
